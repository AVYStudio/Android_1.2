<resources>
    <!-- Base application theme. -->
    <style name="Theme.Android_12" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Android_12.DarkTheme">
        <item name="colorPrimary">#262626</item>
        <item name="colorPrimaryVariant">@color/custom_black</item>
    </style>

    <style name="Theme.Android_12.LightTheme">
        <item name="colorPrimary">#8FC6F1</item>
        <item name="colorPrimaryVariant">#6B9DC3</item>
        <item name="android:navigationBarColor">#6B9DC3</item>
        <item name="colorOnPrimary">@color/custom_black</item>
    </style>
</resources>